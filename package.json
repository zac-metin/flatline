{
  "name": "boilerplate-redux-async",
  "version": "0.1.0",
  "description": "React Redux with simple async action",
  "main": "server/index",
  "scripts": {
    "start": "nodemon --watch server server/index & webpack --watch",
    "eslint": "eslint src",
    "knex": "knex",
    "postinstall": "knex migrate:latest",
    "test": "ava  -v 'tests/**/*.test.js'",
